{
  "name": "fastfoodfast",
  "version": "1.0.0",
  "engines": {
    "node": "8.11.2"
  },
  "description": "\"A web app to order fast food in a restaurant\"",
  "main": "app.js",
  "scripts": {
    "test": "nyc mocha --compilers js:babel-core/register --exit",
    "start": "node bin/dev",
    "clean": "rm -rf dist",
    "build": "npm run clean && mkdir dist && babel server -s -d dist",
    "production": "npm run build && node bin/production",
    "server": "nodemon server/app.js --exec babel-node",
    "script": "babel-node server/app.js",
    "cover": "istanbul cover _mocha",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CodenNerd/FastFoodFast.git"
  },
  "author": "AbdulAzeez Atanda",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/CodenNerd/FastFoodFast/issues"
  },
  "homepage": "https://github.com/CodenNerd/FastFoodFast#readme",
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.18.3",
    "chai": "^4.1.2",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "joi": "^13.6.0",
    "jsonwebtoken": "^8.3.0",
    "make-runnable": "^1.3.6",
    "mocha": "^5.2.0",
    "moment": "^2.22.2",
    "pg": "^7.4.3",
    "supertest": "^3.3.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "coveralls": "^3.0.2",
    "eslint": "^4.9.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "istanbul": "^0.4.5",
    "nyc": "^13.0.1"
  }
}
